{
  "name": "tviewer",
  "version": "1.2.2",
  "author": "Jeong Seongmoon msalt.net",
  "homepage": "https://github.com/msaltnet/T.Viewer",
  "description": "Cross Platform Tizen Log Viewer - T.Viewer",
  "license": "MIT",
  "scripts": {
    "serve-fake": "vue-cli-service electron:serve --mode fake",
    "serve": "vue-cli-service electron:serve",
    "build": "vue-cli-service electron:build",
    "test": "npm run lint && vue-cli-service test:unit",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "ace-builds": "^1.4.7",
    "core-js": "^3.1.2",
    "electron-store": "^6.0.0",
    "electron-window-state": "^5.0.3",
    "mousetrap": "^1.6.5",
    "typeface-roboto": "0.0.75",
    "vue": "^2.6.10",
    "vuetify": "^2.1.0"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.4",
    "@vue/cli-plugin-unit-jest": "^4.0.5",
    "@vue/cli-service": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "electron": "^11.5.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-cli-plugin-vuetify": "^1.1.1",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "eslintIgnore": [
    "/tests/mock/*",
    "/tests/jest*"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
